# Dashboard: traffic, verify conversions, ladder participation correlation, appeal volume, lookup latencies

title: Integrity Page Marketing Dashboard

templating:
  listView:
    panelAmt: 5

panels:
  - title: Traffic Overview
    panelId: 1
    datasource: prometheus
    graphType: timeseries
    gridPos:
      h: 4
      w: 24
    style: dark
    options:
      stacked: false
      showLegend: true
      legend: bottom
      yAxis:
        min: 0
        format: short
    query: sum(rate(traffic_total[1h])) by (job)

  - title: Conversions Verification
    panelId: 2
    datasource: prometheus
    graphType: timeseries
    gridPos:
      h: 4
      w: 8
    style: dark
    options:
      stacked: false
      showLegend: true
      legend: bottom
      yAxis:
        min: 0
        format: short
    query: sum(rate(conversions_verified[1h])) by (job)

  - title: Ladder Participation Correlation
    panelId: 3
    datasource: prometheus
    graphType: scatter
    gridPos:
      h: 4
      w: 8
