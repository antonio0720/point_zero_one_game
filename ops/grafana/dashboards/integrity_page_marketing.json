    queries:
      - query: sum(rate(ladder_participation[1h])) by (job)
        title: Ladder Participation

      - query: sum(rate(appeal_volume[1h])) by (job)
        title: Appeal Volume

  - title: Appeal Volume Overview
    panelId: 4
    datasource: prometheus
    graphType: timeseries
    gridPos:
      h: 4
      w: 8
